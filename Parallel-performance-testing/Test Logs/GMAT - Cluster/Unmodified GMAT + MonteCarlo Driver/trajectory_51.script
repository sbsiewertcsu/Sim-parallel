Create Spacecraft S;
S.DateFormat = UTCGregorian;
S.Epoch = '01 Jan 2025 12:00:00.000';
S.CoordinateSystem = EarthMJ2000Eq;
S.DisplayStateType = Keplerian;
S.SMA = 7376.88;
S.ECC = 0.0;
S.INC = 28.5;
S.RAAN = 0;
S.AOP = 0;
S.TA = 0;

S.DryMass = 200;
S.Cd = 2.16917;
S.DragArea = 8.9197;

Create ForceModel FM;
FM.CentralBody = Earth;
FM.PrimaryBodies = {Earth};
FM.Drag = Exponential;
FM.SRP  = Off;

Create Propagator Prop;
Prop.FM = FM;
Prop.Type = RungeKutta89;
Prop.InitialStepSize = 60;
Prop.Accuracy = 1e-12;
Prop.MinStep = 0.001;
Prop.MaxStep = 600;

Create ReportFile R;
R.Filename = '/user/home/jbcollins/gmat-ieee-aerospace-26/application/bin/MonteCarlo/traj_51.csv';
R.Precision = 15;
R.WriteHeaders = false;

BeginMissionSequence;
Report R S.A1ModJulian S.Altitude;
Propagate Prop(S) { S.Altitude = 122, S.ElapsedDays = 90 };
Report R S.A1ModJulian S.Altitude;
